---

import BaseLayout from '../layouts/base.astro';
import WeekNews from '../components/WeekNews.astro';
import issues from '../issues.json';

---

<BaseLayout>
	{issues.sort((x, y) => x.period[0] < y.period[0] ? 1 : -1).filter(i => i.newsHtml !== "").map((issue) => (
		<WeekNews {issue} />
	))}
</BaseLayout>